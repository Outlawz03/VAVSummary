        <Script Name="RCManager" OnDocumentLoad="load"><![CDATA[global.RCManager = {
	init: function(node) {
		this.per = 20;
		// get containers
		this.containers = [];
		var rcView = node.getChildByName("RCView");
		
		this.canvasWidth = +rcView.getAttribute("Width");
		this.canvasHeight = +rcView.getAttribute("Height");
			
		this.activeContainer = 0;	
		
		var containerList = rcView.getChildNodes();
		this.containers = _.map(containerList, function(){return Object.create(global.Container);});
		_.each(this.containers, function(node, index){node.init(containerList.item(index));});
		
		this.lower = node.getChild("Lower");
		this.higher = node.getChild("Higher");
		
		this.lower.setAttribute("Visibility", "Hidden");
		this.higher.setAttribute("Visibility", "Hidden");
		
		// get Template
		this.template = node.getChildByName("RoomController");
		
		this.rcs = {};		
		
		this.highest = 1;
	},
	
	update: function(e) {
		var RCName = String(e.getTarget().getParentNode().getAttribute("Name")),
			name = String(e.getTarget().getAttribute("Name"));
		
		if(RCName.slice(0,2) === "RC") {
			// check if the RC exists
			var rcNum = +RCName.slice(-2);
			this.highest = Math.max(rcNum, this.highest);
			this.higher.setAttribute("Visibility", this.activeContainer === (Math.ceil(this.highest / this.per) - 1) ? "Hidden" : "Visible");
			if(!_.has(this.rcs, rcNum)) {
				this.rcs[rcNum] = Object.create(global.RCObject);					
				this.rcs[rcNum].init({	model: this.template, 
										num: rcNum,
										perScreen: this.per,
										parent: this.containers[Math.floor((rcNum - 1) / this.per)].node, 
										canvasWidth: this.canvasWidth,
										canvasHeight: this.canvasHeight
									});
			}
			
			// update rc display
			this.rcs[rcNum].update(e);
		}
	},
	
	click: function(e) {
		var target = e.getTarget(),
			parent = target.getParentNode(),
			name = String(target.getAttribute("Name")),
			parentName = String(parent.getAttribute("Name"));
		
		if(name === "Lower" || name === "Higher") {
			if(name === "Lower") {
				this.activeContainer = Math.max(this.activeContainer - 1, 0);	
			} else if(name === "Higher") {
				this.activeContainer = Math.min(this.activeContainer + 1, Math.ceil(this.highest / this.per) - 1);				
			}		
			
			this.lower.setAttribute("Visibility", this.activeContainer === 0 ? "Hidden" : "Visible");
			this.higher.setAttribute("Visibility", this.activeContainer === (Math.ceil(this.highest / this.per) - 1) ? "Hidden" : "Visible");
			
			for(var i = 0, max = this.containers.length; i < max; i += 1) {
				this.containers[i].update(false);
			}
			this.containers[this.activeContainer].update(true);
		}	
		var num = +parentName.slice(-2);
		if(_.has(this.rcs, num)) {
			this.rcs[num].click(e);
		}
	}
};

function load(evt)
{
}]]></Script>
        <Script Name="SE8Manager" OnDocumentLoad="load"><![CDATA[global.SE8Manager = {
	init: function(node) {		
		var children = global.getChildrenByAttribute(node, "SubModel");
			
		this.models = {};
		for(var i = 0, max = children.length; i < max; i += 1) {
			var child = children[i];
			if(child.hasAttribute("SubModel")) {
				var model = String(child.getAttribute("SubModel")),
					ctrl = String(child.getAttribute("Ctrl"));

				this.models[model] = Object.create(eval("global." + ctrl));
				this.models[model].init(child);
			}
		}
		
		this.values = {};
		this.active = null;
		
		this.views = {};
		var viewChildren = node.getChild("Views").getChildNodes();
		for(var i = 0, max = viewChildren.length; i < max; i += 1) {
			var child = viewChildren.item(i),
				name = child.getAttribute("Name");
			this.views[name] = Object.create(global.Component);
			this.views[name].init(child);
		}
		
		this.showView("Monitor");
		this.binds = {};
	},	
	update: function(e) {
		var bind = e.getTarget(),
			parentName = String(bind.getParentNode().getAttribute("Name")),
			bindName = String(bind.getAttribute("Name")),
			attbList = bind.attributes;
			
		if(!_.has(this.values, parentName)) {
			this.values[parentName] = {};
			this.binds[parentName] = {};
		}
		
		this.values[parentName][bindName] = {value: e.getValue(), units: e.getUnit(), name: bindName, attbs: attbList};
		
		this.binds[parentName][bindName] = Object.create(global.Bind);
		this.binds[parentName][bindName].init(bind);
			
		if(parentName === this.active) {	
			for(var i = 0, max = attbList.length; i < max; i += 1) {
				var attb = attbList.item(i).nodeName;
				if(this.models.hasOwnProperty(attb)) {
					this.models[attb].update(this.values[parentName][bindName]);
				}
			}
		}
	},
	click: function(e) {
		var target = e.getTarget(),
			parentNode = target.getParentNode(),
			targetName = String(target.getAttribute("Name")),
			parentName = String(parentNode.getAttribute("Name")),
			attbList = target.attributes;;

		if(_.has(this.values, parentName)) {
			this.active = parentName;
			this.sendValues();
		}
		if(_.has(this.views, targetName)) {
			this.showView(targetName);
		}
		
		for(var i = 0, max = attbList.length; i < max; i += 1) {
			var attb = String(attbList.item(i).nodeName).split("_")[0],
				event = String(attbList.item(i).nodeName).split("_")[1];

			if(event === "click") {
//				alert(attb);
				if(this.models.hasOwnProperty(attb) && this.active !== null && this.binds[this.active].hasOwnProperty(targetName)) {
					if(!global.isUndefined(this.models[attb].click)) {
//						alert(attb);
						this.models[attb].click(this.binds[this.active][targetName]);
					}
	//				if(!this.models[attb].click(this.binds[this.active][targetName])) {
	//					var name = targetName.split(".")[0];
	//					if(this.binds[this.active].hasOwnProperty(name)) {
	//						this.models[attb].click(this.binds[this.active][name]);
	//					}
	//				}
				}
			}
		}
	},
	over: function(e) {
		var attbList = e.getTarget().attributes;;
		
		for(var i = 0, max = attbList.length; i < max; i += 1) {
			var attb = String(attbList.item(i).nodeName).split("_")[0],
				event = String(attbList.item(i).nodeName).split("_")[1];
			if(event === "over") {			
				if(this.models.hasOwnProperty(attb) && this.active !== null) {
					if(!global.isUndefined(this.models[attb].over)) {
						this.models[attb].over();
					}
				}
			}
		}
	},
	out: function(e) {
		var attbList = e.getTarget().attributes;;
		
		for(var i = 0, max = attbList.length; i < max; i += 1) {
			var attb = String(attbList.item(i).nodeName).split("_")[0],
				event = String(attbList.item(i).nodeName).split("_")[1];
			if(event === "out") {			
				if(this.models.hasOwnProperty(attb) && this.active !== null) {
					if(!global.isUndefined(this.models[attb].out)) {
						this.models[attb].out();
					}
				}
			}
		}
	},
	down: function(e) {
		var attbList = e.getTarget().attributes;;
		
		for(var i = 0, max = attbList.length; i < max; i += 1) {
			var attb = String(attbList.item(i).nodeName).split("_")[0],
				event = String(attbList.item(i).nodeName).split("_")[1];
			if(event === "down") {			
				if(this.models.hasOwnProperty(attb) && this.active !== null) {
					if(!global.isUndefined(this.models[attb].down)) {
						this.models[attb].down();
					}
				}
			}
		}
	},
	sendValues: function() {
		for(var key in this.values[this.active]) {
			var value = this.values[this.active][key];
			for(var i = 0, max = value.attbs.length; i < max; i += 1) {
				var attb = value.attbs.item(i).nodeName;
				
				if(_.has(this.models, attb)) {
					this.models[attb].update(value);
				}
			}
		}
	},
	showView: function(sName) {
		for(var key in this.views) {
			if(_.has(this.views, key)) {
				var view = this.views[key];
				view.setVisibility(false);
			}
		}
		this.views[sName].setVisibility(true);
	}
};

function load(evt) {
}]]></Script>
        <Script Name="RCObject" OnDocumentLoad="load"><![CDATA[global.RCObject = {
	init: function(props) {
		// create clone
		this.node = props.model.cloneNode(true);
		
		// attach to correct container
		props.parent.appendChild(this.node);
		
		//set RC Name for click events
		this.node.setAttribute("Name", "RC_" + (props.num < 10 ? "0" : "") + props.num);
		
		// make clone visible
		this.node.setAttribute("Visibility", "Visible");
		
		this.screenNum = props.num - (Math.floor((props.num - 1) / props.perScreen) * props.perScreen);		
		
		// position clone
		var width = +this.node.getAttribute("Width"),
			height = +this.node.getAttribute("Height"),
			row = Math.floor(Math.sqrt(props.perScreen)),
			col = Math.floor(props.perScreen / row),
			colWidth = props.canvasWidth / col,
			rowHeight = props.canvasHeight / row,
			cellRow = Math.floor((this.screenNum - 1) / col),
			cellColumn = (this.screenNum - 1) % col;
			cellTop = cellRow * rowHeight,
			cellLeft = cellColumn * colWidth;
		
		this.node.setAttribute("Top", cellTop + (rowHeight - height) / 2);
		this.node.setAttribute("Left", cellLeft + (colWidth - width) / 2);
		
		this.models = {};
		var children = this.node.getChildNodes();
		for(var i = 0, max = children.length; i < max; i += 1) {
			var child = children.item(i);
			if(child.hasAttribute("SubModel")) {
				var model = String(child.getAttribute("SubModel")),
					ctrl = String(child.getAttribute("Ctrl"));
				this.models[model] = Object.create(eval("global." + ctrl));
				this.models[model].init(child);
			}
		}
		
		this.binds = {};
	},
	update: function(e) {
		var bindNode = e.getTarget(),
			bindName = bindNode.getAttribute("Name"),
			attbList = bindNode.attributes,
			signal = {value: e.getValue(), units: e.getUnit(), name: bindName};
			
		this.binds[bindName] = Object.create(global.Bind);
		this.binds[bindName].init(bindNode);
		
		for(var i = 0, max = attbList.length; i < max; i += 1) {
			var attb = attbList.item(i).nodeName;
			if(this.models.hasOwnProperty(attb)) {
				this.models[attb].update(signal);
			}
		}
	},
	click: function(e) {
		var target = e.getTarget(),
			name = target.getAttribute("Name"),
			attbList = target.attributes;
		
		for(var i = 0, max = attbList.length; i < max; i += 1) {
			var attb = String(attbList.item(i).nodeName).split("_")[0],
				event = String(attbList.item(i).nodeName).split("_")[1];
			if(event === "click") {
				if(this.models.hasOwnProperty(attb) && this.binds.hasOwnProperty(name)) {
					this.models[attb].click(this.binds[name]);
				}
			}
		}
	}
};

function load(evt) {
}]]></Script>
