        <Script Name="OccBoxesCtrl" OnDocumentLoad="load"><![CDATA[global.OccBoxesCtrl = {
	init: function(node) {
		this.values = {};
		this.txtUU = node.getChild("UU").getChild("Value");
		this.txtRU = node.getChild("RU").getChild("Value");
		this.txtUO = node.getChild("UO").getChild("Value");
		this.txtRO = node.getChild("RO").getChild("Value");
		this.txtTotal = node.getChild("Total").getChild("Value");
	},
	update: function(e) {
		var parentName = String(e.getTarget().getParentNode().getAttribute("Name"));
		
		this.values[parentName] = +e.getValue();
		
		var total = 0,
			uu = 0,
			uo = 0,
			ru = 0,
			ro = 0;
			
		for(var key in this.values) {
			var val = this.values[key];
			total += 1;
			switch(val) {
			case 1:
				ro += 1;
				break;
			case 2:
				uu += 1;
				break;
			case 3:
				uo += 1;
				break;
			case 4:
				ru += 1;
				break;
			default:
			}
		}
		
		this.txtUU.setAttribute("Content", uu);
		this.txtRU.setAttribute("Content", ru);
		this.txtUO.setAttribute("Content", uo);
		this.txtRO.setAttribute("Content", ro);
		this.txtTotal.setAttribute("Content", total);
	}
};

function load(evt) {
}]]></Script>
        <Script Name="SptCtrl" OnDocumentLoad="load"><![CDATA[global.SptCtrl = {
	init: function(node) {
		this.node = node;
		this.values = {
			"Effective Occupancy"			: {value: 0, units: null},
			"System Mode"			: {value: 0, units: null},
			"PI Cooling Demand"			: {value: 0, units: null},
			"PI Heating Demand"			: {value: 0, units: null},
			"Unoccupied Cool Setpoint"		: {value: 0, units: null},
			"Unoccupied Heat Setpoint"		: {value: 0, units: null},
			"Standby Cool Setpoint"	: {value: 0, units: null},
			"Standby Heat Setpoint"	: {value: 0, units: null},
			"Occupied Cool Setpoint"		: {value: 0, units: null},
			"Occupied Heat Setpoint"		: {value: 0, units: null}
		};

		this.txt = Object.create(global.Text);
		this.txt.init(this.node);
	},
	update: function(sig) {
		var name = String(sig.name);
		if(_.has(this.values, name)) {		
			this.values[name].value = +sig.value;
			this.values[name].units = String(sig.units);
		
			this.render();
		}
	},
	updateAll: function(sigs) {
		for(var key in this.values) {
			if(_.has(sigs, key)) {
				this.values[key].value = +sigs[key].value;
				this.values[key].units = String(sigs[key].units);
			}
		}
		this.render();
	},
	render: function() {
		var content = "";
		switch(this.values["Effective Occupancy"].value) {
		case 1:
		case 3:
			if(this.values["PI Heating Demand"].value > 0 || this.values["System Mode"].value === 4) {
				content = this.values["Occupied Heat Setpoint"].value.toFixed(1) + " " + this.values["Occupied Heat Setpoint"].units;
			} else {
				content = this.values["Occupied Cool Setpoint"].value.toFixed(1) + " " + this.values["Occupied Cool Setpoint"].units;
			}
			break;
		case 2:
			if(this.values["PI Heating Demand"].value > 0  || this.values["System Mode"].value === 4) {
				content = this.values["Unoccupied Heat Setpoint"].value.toFixed(1) + " " + this.values["Unoccupied Heat Setpoint"].units;
			} else {
				content = this.values["Unoccupied Cool Setpoint"].value.toFixed(1) + " " + this.values["Unoccupied Cool Setpoint"].units;
			}
			break;
		case 4:
			if(this.values["PI Heating Demand"].value > 0  || this.values["System Mode"].value === 4) {
				content = this.values["Standby Heat Setpoint"].value.toFixed(1) + " " + this.values["Standby Heat Setpoint"].units;
			} else {
				content = this.values["Standby Cool Setpoint"].value.toFixed(1) + " " + this.values["Standby Cool Setpoint"].units;
			}
			break;
		default:
			content = "---Â°F";
		}

		this.txt.setContent(content);
	}
};

function load(evt) {
}]]></Script>
        <Script Name="OccCtrl" OnDocumentLoad="load"><![CDATA[global.OccCtrl = {
	init: function(node) {
		this.node = node;
		this.values = {
			"Low Battery Alarm"	: 0,
			"Effective Occupancy"	: 0,
			"Occupancy Command"	: 0,
		};
		
		this.fills = ["#FF0000", "#339999", "#87D300", "#E47F00", "#42B4E6"];
		this.strokes = ["#FFFFFF", "#FFFFFF", "#404040", "#FFFFFF", "#FFFFFF"];
		this.contents = ["No Communication", "Rented/Occupied", "Unrented/Unoccupied", "Unrented/Occupied", "Rented/Unoccupied"];

		this.bgd = Object.create(global.Rectangle);
		this.bgd.init(this.node.getChildByName("background"));
		
		this.fgd = Object.create(global.Rectangle);
		this.fgd.init(this.node.getChildByName("innerline"));
		
		this.txt = Object.create(global.Text);
		this.txt.init(this.node.getChildByName("Text"));		
	},
	update: function(sig) {
		var name = String(sig.name);
		if(_.has(this.values, name)) {		
			this.values[name] = +sig.value;
		
			this.render();
		}
	},
	updateAll: function(sigs) {
		for(var key in this.values) {
			if(_.has(sigs, key)) {
				this.values[key] = +sigs[key].value;
			}
		}
		this.render();
	},
	render: function() {
		var content = "";
		var fill = "#404040";
		var stroke = "#FFFFFF";
		if(this.values["Low Battery Alarm"] === 1) {
			fill = "#FFD100";
			content = "Low Battery";
			stroke = "#404040";
		} else {
			fill = this.fills[this.values["Effective Occupancy"]];
			content = this.contents[this.values["Effective Occupancy"]];
			stroke = this.strokes[this.values["Effective Occupancy"]];
		}

		this.txt.setContent(content);
		this.txt.setStroke(stroke);
		this.bgd.setFill(fill);
		this.fgd.setFill(fill);
	}
};

function load(evt) {
}]]></Script>
