    <Script Name="GS" OnDocumentLoad="load" OnMouseClick="click"
        OnMouseDown="down" OnMouseMove="" OnMouseOut="out"
        OnMouseOver="over" OnMouseUp="up" OnSignalChange="change"><![CDATA[var _ = global._;

/**==========================================================================================
 * Helper Functions
 */
global.isUndefined = function(o) {
	if(typeof(o) === "undefined") {
		return true;			
	}
	return false;	
};

global.getBool = function(s) {
	var trueEnum = ["yes", "y", "Yes", "Y", "1", "True", "true", "T", "t", "On", "active"];
	var i = trueEnum.length;
	while(i--) {
		if(String(s) === trueEnum[i]) {
			return true;
		}
	}
	return false
};

//checks if object is of type undefined
if (!Object.isUndefined) {
	Object.isUndefined = function(object) {
		return typeof object === "undefined";
	};
}

//checks if object is part of DOM, should only be used on nodes (good to check against CDATA)
if (!Object.isDOM) {
	Object.isDOM = function(obj) {
		if (!Object.isUndefined(obj) && obj.nodeName) {
	    	switch (obj.nodeType) {
	    		case 1: return 'element';
	    		case 3: return (/\S/).test(obj.nodeValue) ? 'textnode' : 'whitespace';
	    	}
	    }
	    return false;
	};
}

//returns an array children nodes that have a defined attribute, ex: getChildrenByAttribute(view,"Model")
global.getChildrenByAttribute = function(node, attb){
	var v = [];
	if (Object.isDOM(node)) {
		if (node.hasAttribute(attb)) {
			if (node.getAttribute(attb)!="") {			
				v.push(node);
			}
		}
		if (node.hasChildNodes()) {
			var c = node.getChildNodes();			
			for(var i = 0; i < c.length; i++) {
				v = v.concat(global.getChildrenByAttribute(c.item(i), attb));
			}
		}
	}
	return v;
}

if (!Object.create) {
	Object.create = function(base) {
		function F () {};
		F.prototype = base;
		return new F();
	};
}

/**==========================================================================================
 * main Object
 */
var main = {		
	load: function(e) {
		var tgml = e.getCurrentTarget().getOwnerDocument().getDocumentElement(),
			children = global.getChildrenByAttribute(tgml, "Model");
			
		this.models = {};
		for(var i = 0, max = children.length; i < max; i += 1) {
			var child = children[i],
				model = child.getAttribute("Model"),
				ctrl = child.getAttribute("Ctrl");
			this.models[model] = Object.create(eval("global." + ctrl));
			this.models[model].init(child);				
		}
	},
	
	// change event handler
	change: function(e) {
		e.preventDefault();
		var bind = e.getTarget(),
			attbList = bind.attributes;
			
		for(var i = 0, max = attbList.length; i < max; i += 1) {
			var attb = attbList.item(i).nodeName;
			if(this.models.hasOwnProperty(attb)) {
				this.models[attb].update(e);
			}
		}	
	},
	
	// mouse click event handler
	up: function(e) {
		var trigger = e.getTarget(),
			attbList = trigger.attributes;
		
		for(var i = 0, max = attbList.length; i < max; i += 1) {
			var attb = String(attbList.item(i).nodeName).split("_")[0],
				event = String(attbList.item(i).nodeName).split("_")[1];
			if(event === "click") {
				if(this.models.hasOwnProperty(attb)) {
					if(!global.isUndefined(this.models[attb].click)) {
						this.models[attb].click(e);
					}
				}
			}
		}
	},
	
	// mouse click event handler
	down: function(e) {
		var trigger = e.getTarget(),
			attbList = trigger.attributes;
		
		for(var i = 0, max = attbList.length; i < max; i += 1) {
			var attb = String(attbList.item(i).nodeName).split("_")[0],
				event = String(attbList.item(i).nodeName).split("_")[1];
			if(event === "down") {
				if(this.models.hasOwnProperty(attb)) {
					if(!global.isUndefined(this.models[attb].down)) {
						this.models[attb].down(e);
					}
				}
			}
		}
	},
	
	// mouse click event handler
	over: function(e) {
		var trigger = e.getTarget(),
			attbList = trigger.attributes;
		
		for(var i = 0, max = attbList.length; i < max; i += 1) {
			var attb = String(attbList.item(i).nodeName).split("_")[0],
				event = String(attbList.item(i).nodeName).split("_")[1];
			if(event === "over") {
				if(this.models.hasOwnProperty(attb)) {
					if(!global.isUndefined(this.models[attb].over)) {
						this.models[attb].over(e);
					}
				}
			}
		}
	},
	
	// mouse click event handler
	out: function(e) {
		var trigger = e.getTarget(),
			attbList = trigger.attributes;
		
		for(var i = 0, max = attbList.length; i < max; i += 1) {
			var attb = String(attbList.item(i).nodeName).split("_")[0],
				event = String(attbList.item(i).nodeName).split("_")[1];
			if(event === "out") {
				if(this.models.hasOwnProperty(attb)) {
					if(!global.isUndefined(this.models[attb].out)) {
						this.models[attb].out(e);
					}
				}
			}
		}
	},
	
	click: function(e) {
		e.preventDefault();
	}	
};

/**==========================================================================================
 * Event Callback Functions
 */

function load(evt){	
