        <Script Name="SwitchCtrl" OnDocumentLoad="load"><![CDATA[global.SwitchCtrl = {
	init: function(node) {
		this.node = node;
		this.switchAnimate = Object.create(global.Animate);
		this.switchAnimate.init(this.node.getChildByName("SwAni"));
		
		this.bgdAnimate = Object.create(global.Animate);
		this.bgdAnimate.init(this.node.getChildByName("BgAni"));

		this.rightValue = +this.node.getAttribute("RightValue");
		this.leftValue = +this.node.getAttribute("LeftValue");
		this.value = 0;
		this.node.setAttribute("Visibility", "Hidden");
	},
	
	update: function(sig) {		
		this.node.setAttribute("Visibility", "Visible");
		this.value = +sig.value;
		this.switchAnimate.update(this.value === this.leftValue);
